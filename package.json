  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "cypress": "^12.11.0",
    "cypress-mochawesome-reporter": "^3.6.0",
    "cypress-multi-reporters": "^1.6.3",
    "dotenv": "^16.3.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
